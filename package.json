{
  "name": "hi_score",
  "version": "1.1.2",
  "description": "Get off the framework-of-the-month treadmill and take control of your SPA. Hi_Score assembles best-in-class libraries in a feature-module architecture as detailed in 'Single Page Web Applicaitons, JavaScript end-to-end'",
  "keywords": [
    "spa",
    "library",
    "libraries",
    "single",
    "page",
    "web",
    "application",
    "end-to-end"
  ],
  "main": "hi_score",
  "scripts": {
    "build": "bin/buildify cfg/*.buildify",
    "setup": "bin/setup",
    "test": "node_modules/.bin/nodeunit test/xhi_level_0.js",
    "cover": "node_modules/.bin/istanbul cover -x '**/vendor/**' node_modules/.bin/nodeunit test/xhi_level_0.js",
    "covera": "node_modules/.bin/istanbul cover -x '**/vendor/**' node_modules/.bin/nodeunit test/xhi_level_0.js; cat coverage/lcov.info | node_modules/.bin/coveralls"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mmikowski/hi_score.git"
  },
  "author": "Michael S. Mikowski <mike.mikowski@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mmikowski/hi_score/issues"
  },
  "homepage": "https://github.com/mmikowski/hi_score",
  "dependencies": {},
  "devDependencies": {
    "font-awesome"            : "4.7.0",
    "jquery"                  : "3.2.0",
    "jquery.event.dragscroll" : "1.0.0",
    "jquery.event.gevent"     : "1.1.6",
    "jquery.event.ue"         : "1.3.2",
    "jquery.scrolli"          : "1.0.1",
    "jquery.urianchor"        : "1.3.5",
    "open-sans-fontface"      : "1.4.0",
    "powercss"                : "1.3.5",
    "taffydb"                 : "2.7.3",

    "clusterjs"               : "0.7.1",
    "express"                 : "4.15.2",
    "websocket"               : "1.0.22",
    "jsdom"                   : "9.12.0",
    "mongodb"                 : "2.2.25",
    "mysql2"                  : "1.2.0",

    "coveralls"               : "2.12.0",
    "jslint"                  : "0.10.3",
    "istanbul"                : "0.4.5",
    "node-inspector"          : "0.12.10",
    "nodeunit"                : "0.11.0",
    "uglifycss"               : "0.0.25",
    "uglifyjs"                : "2.4.10"
  },
  "maintainers": [
    "Michael S. Mikowski <mike.mikowski@gmail.com>"
  ]
}
