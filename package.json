  "scripts": {
    "10": "bin/xhi_tool 10",
    "11": "bin/xhi_tool 11",
    "12": "bin/xhi_tool 12",
    "13": "bin/xhi_tool 13",
    "14": "bin/xhi_tool 14",
    "15": "bin/xhi_tool 15",
    "16": "bin/xhi_tool 16",
    "17": "...node bin/xhi_tool 17",
    "18": "bin/xhi_tool 18",
    "00_.": "=== HELP",
    "00": "bin/xhi_tool 00",
    "01_.": "=== INSTALL",
    "01_a": "...node preinstall: Run BEFORE the pkg is installed",
    "01": "bin/xhi_tool 01",
    "02_.": "=== SETUP",
    "02_a": "...node install, postinstall: Run AFTER the pkg is installed.",
    "02": "bin/xhi_tool 02",
    "03_.": "=== DESIGN",
    "03__": "...synonym cat /doc/spa-arch.md",
    "03": "bin/xhi_tool 03",
    "04_.": "=== DEVSERVER-START",
    "04_a": "...node prestart",
    "04_1": "...node start",
    "04_2": "...node poststart",
    "04": "bin/xhi_tool 04",
    "05_.": "=== DEVELOP-LINT",
    "05": "bin/xhi_tool 05",
    "06_.": "=== DEVELOP-TEST",
    "06_a": "...node pretest, test, posttest: Run by the npm test command.",
    "06": "bin/xhi_tool 06",
    "07_.": "=== DEVELOP-COVERAGE",
    "07": "bin/xhi_tool 07",
    "08_.": "=== DEVELOP-COMMIT",
    "08_a": "...node preversion: Run BEFORE bumping the pkg version.",
    "08_b": "...node version: Run AFTER bumping the pkg version, but BEFORE commit.",
    "08_1": "...node postversion: Run AFTER bumping the pkg version, and AFTER commit.",
    "08": "bin/xhi_tool 08",
    "09_.": "=== BUILD",
    "09_a": "...node prepare: Run both BEFORE the pkg is packed and published, and on local npm install without any arguments (See below). This is run AFTER prepublish, but BEFORE prepublishOnly.",
    "09": "bin/xhi_tool 09",
    "10_.": "=== PUBLISH",
    "10_a": "...node prepublish: Run BEFORE the pkg is packed and published, as well as on local npm install without any arguments. (See below)",
    "10_b": "...node prepublishOnly: Run BEFORE the pkg is prepared and packed, ONLY on npm publish. (See below.)",
    "10_1": "...node publish",
    "10_2": "...node postpublish: Run AFTER the pkg is published.",
    "11_.": "=== DEVSERVER-RESTART",
    "11_a": "...node prerestart",
    "11_1": "...node restart",
    "11_2": "...node postrestart",
    "11_3": "...node WARN: npm restart will run the stop and start scripts if no restart script is provided.",
    "12_.": "=== DEVSERVER-STOP",
    "12_a": "...node prestop",
    "12_1": "...node stop",
    "12_2": "...node poststop",
    "13_.": "=== DEPLOY",
    "13_a": "...node preshrinkwrap, shrinkwrap, postshrinkwrap: Run by the npm shrinkwrap command.",
    "13_b": "...node prepack: run BEFORE a tarball is packed (on npm pack, npm publish, and when installing git dependencies)",
    "13_1": "...node postpack: Run AFTER the tarball has been generated and moved to its final destination.",
    "14_.": "=== PRODSERVER-START",
    "15_.": "=== PRODSERVER-RESTART",
    "16_.": "=== PRODSERVER-STOP",
    "17_.": "=== FEEDBACK",
    "18_.": "=== UNINSTALL",
    "18_1": "...node preuninstall, uninstall: Run BEFORE the pkg is uninstalled.",
    "18_2": "...node postuninstall: Run AFTER the pkg is uninstalled.",
    "help": "bin/xhi_tool help",
    "coverage": "istanbul cover -x '**/vendor/**' node_modules/.bin/nodeunit test/xhi_level_0.js",
    "make": "bin/buildify cfg/*.buildify",
    "publish-coverage": "istanbul cover -x '**/vendor/**' node_modules/.bin/nodeunit test/xhi_level_0.js; cat coverage/lcov.info | node_modules/.bin/coveralls",
    "requirements-check": "bin/check-node-version",
    "setup": "bin/setup",
    "test": "node_modules/.bin/nodeunit test/xhi_level_0.js"
  },
